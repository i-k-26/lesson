set names utf8;
set foreign_key_check = 0;
drop database if exists logindb3;
create database logindb3;
use logindb3;

drop table if exists user;

create table user(
id int,
login_id varchar(16),
login_pass varchar(16)
);

insert into user values
(1,"taro","111"),
(2,"jiro","222"),
(3,"saburo","333");